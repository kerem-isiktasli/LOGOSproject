# LOGOS Environment Variables
# Copy this file to .env and fill in your values

# Database
DATABASE_URL="file:./data/logos.db"

# Claude API
ANTHROPIC_API_KEY="your-api-key-here"

# Application
NODE_ENV="development"

# Application Mode ('mvp' or 'full')
# MVP mode shows simplified UI with core features only
# Full mode shows all advanced features
LOGOS_MODE="mvp"
